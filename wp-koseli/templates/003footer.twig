 <!--FOOER AREA-->
    <div class="footer-area" id="contact">
        <div class="footer-area-bg" data-stellar-background-ratio="0.6" style="background:rgba(0, 0, 0, 0) url('{{footer_bg.url}}') no-repeat scroll center center / cover"></div>
        <div class="container wow fadeIn">
            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12">
                    <div class="footer-top section-padding text-center">
                        <div class="footer-logo">
                            <a href="#"><img src="{{logo_2.url}}" alt=""></a>
                        </div>
                        <div class="footer-address">
                            <p>{% if language == 'en' %}{{address_finnish}}{% else %}{{address}}{% endif %}</p>
                            <p><a href="mailto:{{email}}">{{email}}</a></p>
                            <p><a href="callto:telephone">{{telephone}}</a></p>
                        </div>
                        <div class="footer-social-bookmark">
                            <ul>
                                <li><a href="{{facebook}}" target="_blank"><i class="fa fa-facebook"></i></a></li>
                                <li><a href="{{twitter}}" target="_blank"><i class="fa fa-twitter"></i></a></li>
                                <!-- <li><a href="#"><i class="fa fa-dribbble"></i></a></li>
                                <li><a href="#"><i class="fa fa-google-plus"></i></a></li>
                                <li><a href="#"><i class="fa fa-flickr"></i></a></li>
                                <li><a href="#"><i class="fa fa-pinterest"></i></a></li>
                                <li><a href="#"><i class="fa fa-skype"></i></a></li>
                            </ul> -->
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12">
                    <div class="footer-menu">
                        <ul>
                            <li class="nav-item" ><a data-toggle="modal" data-target="#privacy">
                                    {% if language == 'en' %}
                                        Tietosuojakäytäntö
                                    {% else %}
                                        Privacy Policy 
                                    {% endif %}
                            </a></li>
                            <li class="nav-item" ><a data-toggle="modal" data-target="#terms">
                                {% if language == 'en' %}
                                        Käyttöehdot
                                    {% else %}
                                        Terms of Use
                                    {% endif %}
                            </a></li>
                            {#{% for keys,item in foot_menu.get_items %}
                                <li class="nav-item {{item.classes | join(' ')}}">
                                    <a class="nav-link" href="{{item.get_link}}">{{item.title}}</a>
                                </li>
                            {% endfor %} #}
                                <div id="privacy" class="modal fade" role="dialog">
                                      <div class="modal-dialog modal-lg">

                                        <!-- Modal content-->
                                        <div class="modal-content" style="border-radius:0px;">
                                          <div class="modal-header">
                                            <button type="button" class="close" data-dismiss="modal">&times;</button>
                                            <h4 class="modal-title">
                                                {% if language == 'en' %}
                                                    Tietosuojakäytäntö
                                                {% else %}
                                                    Privacy Policy 
                                                {% endif %}</h4>
                                          </div>
                                          <div class="modal-body">
                                            <p>
                                                {% if language == 'en' %}
                                                    {{privacy_finnish}}
                                                {% else %}
                                                    {{privacy}}
                                                {% endif %}
                                            </p>
                                          </div>
                                          <div class="modal-footer">
                                            <button type="button" class="btn btn_rest btn-default" data-dismiss="modal">Close</button>
                                          </div>
                                        </div>
                                      </div>
                                </div>
                                <div id="terms" class="modal fade" role="dialog">
                                      <div class="modal-dialog modal-lg">

                                        <!-- Modal content-->
                                        <div class="modal-content" style="border-radius:0px;">
                                          <div class="modal-header">
                                            <button type="button" class="close" data-dismiss="modal">&times;</button>
                                            <h4 class="modal-title">
                                                {% if language == 'en' %}
                                                    Käyttöehdot
                                                {% else %}
                                                    Terms of Use
                                                {% endif %}
                                            </h4>
                                          </div>
                                          <div class="modal-body">
                                            <p>
                                            {% if language == 'en' %}
                                                {{tos_finnish}}
                                            {% else %}
                                                {{tos}}
                                            {% endif %}
                                        </p>
                                          </div>
                                          <div class="modal-footer">
                                            <button type="button" class="btn btn_rest btn-default" data-dismiss="modal">Close</button>
                                          </div>
                                        </div>
                                      </div>
                                </div>
                        </ul>
                    </div>
                </div>
                <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12">
                    <div class="footer-copyright">
                        {% if language == 'en' %}
                            <p>Tekijänoikeus &copy; {{"now"|date('Y')}} <a href="#">{{ site.name }}. </a>Kaikki oikeudet pidätetään.</p>
                        {% else %}
                            <p>Copyright &copy; {{"now"|date('Y')}} <a href="#">{{ site.name }}. </a> All Right Reserved.</p>
                        {% endif %}

                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--FOOER AREA END-->


    <!--====== SCRIPTS JS ======-->
    <script src="{{theme.link}}/assets/js/vendor/jquery-1.12.4.min.js"></script>
    <script src="{{theme.link}}/assets/js/vendor/bootstrap.min.js"></script>

    <!--====== PLUGINS JS ======-->
    <script src="{{theme.link}}/assets/js/vendor/jquery.easing.1.3.js"></script>
    <script src="{{theme.link}}/assets/js/vendor/jquery-migrate-1.2.1.min.js"></script>
    <script src="{{theme.link}}/assets/js/jquery.pogo-slider.js"></script>
    <script src="{{theme.link}}/assets/js/owl.carousel.min.js"></script>
    <script src="{{theme.link}}/assets/js/stellar.js"></script>
    <script src="{{theme.link}}/assets/js/instafeed.min.js"></script>
    <script src="{{theme.link}}/assets/js/datepicker.min.js"></script>
    <script src="{{theme.link}}/assets/js/timepicker.min.js"></script>
    <script src="{{theme.link}}/assets/js/wow.min.js"></script>
    <script src="{{theme.link}}/assets/js/jquery.magnific-popup.min.js"></script>
    <script src="{{theme.link}}/assets/js/jquery.sticky.js"></script>
    <script src="{{theme.link}}/assets/js/jquery.cookie.js"></script>

    <!--===== ACTIVE JS=====-->
    <script src="{{theme.link}}/assets/js/main.js"></script>
    <script>
        var feed = new Instafeed({
            get: '{{instagram_user}}',
            userId: {{instagram_id}},
            accessToken: '{{instagram_access_token}}',
            target: 'instagram',
            limit: {{no_to_show}}, //max 60 images..
            resolution: 'standard_resolution',
            after: function () {
                var el = document.getElementById('instagram');
                if (el.classList)
                    el.classList.add('show');
                else
                    el.className += ' ' + 'show';
        }
    });
    feed.run();
    
    </script>
</body>



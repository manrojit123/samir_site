    overflow: hidden;
    margin: 0;
}
.side_image img{
        background-size: cover;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    float: right;
    overflow: hidden;
    margin: 0;
}
